n\n  var rand = Game.randomInRange(0, scoreSum);\n  var selected;\n  for (var enemy of pop) {\n    if (rand - enemy.score < 0) {\n      selected = enemy;\n      break;\n    } else {\n      rand -= enemy.score;\n    }\n  }\n  var child = new Enemy();\n  child.nn.model = selected.nn.copy();\n  child.nn.mutate(0.05);\n  return child;\n}\n\nexport function getFurthestPlayer() {\n  var highestscore = 0;\n  var target;\n  for (var p of game.population) {\n    if (p.score > highestscore) {\n      highestscore = p.score;\n      target = p;\n    }\n  }\n  return target;\n}\n\nexport function managePopulation() {\n  if (game.population.length === 0) {\n    console.log(\"new generation\");\n    game.restartGame();\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"8fb23902eb21e764290559a0084f7c3f","cacheData":{"env":{}}}